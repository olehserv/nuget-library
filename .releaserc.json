  "branches": ["main"],
  "tagFormat": "v${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",

    ["@semantic-release/release-notes-generator", {
      "writerOpts": {
        "footerPartial": "## NuGet\n- All packages: https://www.nuget.org/packages?q=olehserv.fulcrum-software-test\n"
      }
    }],
    
    "@semantic-release/github"
  ]
}